{
  "expo": {
    "name": "SpotifyHistoryApp",
    "slug": "SpotifyHistoryApp",
    "scheme": "spotify-history",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.familiarcat.SpotifyHistoryApp"
    },
    "android": {
      "package": "com.familiarcat.SpotifyHistoryApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "279dd093-d5fa-4a0f-a492-7a458be69b25"
      }
    }
  }
}
